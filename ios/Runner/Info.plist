		<key>KibleYonu</key>
		<string>Kıble yönünü doğru hesaplamak için hassas konum bilginize ihtiyaç duyuyoruz</string>
	</dict>
	<!-- Pusula sensörü izni -->
	<key>NSMotionUsageDescription</key>
